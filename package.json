{
  "name": "study-buddy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run db:generate && next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate --schema=./src/db/prisma/schema.prisma",
    "db:push": "prisma db push --schema=./src/db/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=./src/db/prisma/schema.prisma",
    "db:pull": "prisma db pull --schema=./src/db/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./src/db/prisma/schema.prisma",
    "db:migrate reset": "prisma migrate reset --schema=./src/db/prisma/schema.prisma"
  },
  "dependencies": {
    "@google/generative-ai": "^0.22.0",
    "@prisma/client": "^6.4.1",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@stackframe/stack": "^2.7.16",
    "async_hooks": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "framer-motion": "^12.4.5",
    "lucide-react": "^0.475.0",
    "next": "15.1.7",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20.17.19",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "prisma": "^6.4.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3"
  }
}
